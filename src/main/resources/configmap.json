{
  "server": {
    "port": 8080
  },
  "kube": {
    "apiOrigin": "https://kubernetes.default.svc:6443",
    "tokenPath": "./.kube/token",
    "caCertPath": "./.kube/ca.crt",
    "jwksEndpoint": "/openid/v1/jwks",
    "openIdConfigurationEndpoint": "/.well-known/openid-configuration",
    "externalJwksUri": "https://kubernetes.default.svc.k8s.kperczynski.click/openid/v1/jwks",
    "sslTrustAll": false
  },
  "ec2": {
    "currentNode": "$env:CURRENT_KUBERNETES_NODE",
    "apiOrigin": "http://169.254.169.254",
    "ttlSeconds": 21600
  }
}
